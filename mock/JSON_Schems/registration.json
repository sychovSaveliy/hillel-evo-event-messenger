  "post-data": {
    "username": {
      "type": "string",
      "maxLength": 20,
      "pattern": "[-a-z0-9!#$%&'*+/=?^_`{|}~]+(\\.[-a-z0-9!#$%&'*+/=?^_`{|}~]+)*"
    },
    "password": {
      "type": "string",
      "maxLength": 20,
      "pattern": "[a-z\\d~+-]+"
    },
    "e-mail": {
      "type": "string",
      "maxLength": 30,
      "pattern": "/^[a-zA-Z0-9.!#$%&'*+\/=​?^_`{|}~-]+@​[a-zA-Z0-9](?:​[a-zA-Z0-9-]{0,61}[a-zA-Z0-9])?(?:​\\.[a-zA-Z0-9](?:​[a-zA-Z0-9-]{0,     61}[a-zA-Z0-9])?)*$/"
    }
  }
}
